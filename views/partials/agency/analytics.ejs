  <!-- Analytics Section -->
  <section class="agency-dashboard__analytics" id="analytics">
    <div class="agency-dashboard__analytics-container">
      <div class="agency-dashboard__analytics-header">
        <h2 class="agency-dashboard__analytics-title">Analytics & Insights</h2>
        <p class="agency-dashboard__analytics-subtitle">Track your application pipeline and performance metrics</p>
          </div>

      <div class="agency-dashboard__analytics-content">
        <!-- Loading State -->
        <div class="agency-dashboard__analytics-loading" id="analytics-loading">
          <div style="text-align: center; padding: 3rem;">
            <div style="margin-bottom: 1rem; color: var(--agency-text-secondary);">Loading analytics...</div>
            <div style="font-size: 0.875rem; color: var(--agency-text-tertiary);">This may take a moment</div>
        </div>
      </div>

        <!-- Error State -->
        <div class="agency-dashboard__analytics-error" id="analytics-error" style="display: none;">
          <div style="text-align: center; padding: 3rem; color: var(--agency-text-tertiary);">
            <div style="margin-bottom: 0.5rem; font-size: 0.875rem;">Analytics unavailable</div>
            <div style="font-size: 0.8125rem;">Unable to load analytics data at this time</div>
          </div>
        </div>

        <!-- Analytics Content -->
        <div class="agency-dashboard__analytics-main" id="analytics-content" style="display: none;">
          <!-- Key Metrics Grid -->
          <div class="agency-dashboard__analytics-grid">
            <div class="agency-dashboard__analytics-card">
              <div class="agency-dashboard__analytics-card-header">
                <h3 class="agency-dashboard__analytics-card-title">Total Applications</h3>
              </div>
              <div class="agency-dashboard__analytics-card-value" id="analytics-total"><%= safeStats.total %></div>
              <div class="agency-dashboard__analytics-card-trend">
                <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                  <polyline points="23 6 13.5 15.5 8.5 10.5 1 18"/>
                  <polyline points="17 6 23 6 23 12"/>
                </svg>
                <span class="agency-dashboard__analytics-card-trend-text" id="analytics-total-trend">+12% from last week</span>
              </div>
            </div>

            <div class="agency-dashboard__analytics-card">
              <div class="agency-dashboard__analytics-card-header">
                <h3 class="agency-dashboard__analytics-card-title">Scout Profile Views</h3>
              </div>
              <div class="agency-dashboard__analytics-card-value" id="analytics-profile-views">1,204</div>
              <div class="agency-dashboard__analytics-card-trend">
                <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                  <polyline points="23 6 13.5 15.5 8.5 10.5 1 18"/>
                  <polyline points="17 6 23 6 23 12"/>
                </svg>
                <span class="agency-dashboard__analytics-card-trend-text">+8% from last week</span>
              </div>
            </div>

            <div class="agency-dashboard__analytics-card">
              <div class="agency-dashboard__analytics-card-header">
                <h3 class="agency-dashboard__analytics-card-title">Acceptance Rate</h3>
              </div>
              <div class="agency-dashboard__analytics-card-value" id="analytics-acceptance-rate">
                <%= safeStats.total > 0 ? ((safeStats.accepted / safeStats.total) * 100).toFixed(1) : 0 %>%
              </div>
              <div class="agency-dashboard__analytics-card-trend agency-dashboard__analytics-card-trend--negative">
                <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                  <polyline points="23 18 13.5 8.5 8.5 13.5 1 6"/>
                  <polyline points="17 18 23 18 23 12"/>
                </svg>
                <span class="agency-dashboard__analytics-card-trend-text">-2% from last week</span>
              </div>
            </div>

            <div class="agency-dashboard__analytics-card">
              <div class="agency-dashboard__analytics-card-header">
                <h3 class="agency-dashboard__analytics-card-title">Avg. Response Time</h3>
              </div>
              <div class="agency-dashboard__analytics-card-value" id="analytics-response-time">4h 12m</div>
              <div class="agency-dashboard__analytics-card-subtext">Same as average</div>
            </div>
          </div>

          <!-- Charts Section -->
          <div class="agency-dashboard__analytics-charts">
            <div class="agency-dashboard__analytics-chart-card">
              <h3 class="agency-dashboard__analytics-chart-title">Weekly overview of incoming talent traffic.</h3>
              <div class="agency-dashboard__analytics-chart-container" id="analytics-traffic-chart">
                <!-- Chart will be rendered here -->
              </div>
            </div>

            <div class="agency-dashboard__analytics-chart-card">
              <h3 class="agency-dashboard__analytics-chart-title">Distribution by experience level.</h3>
              <div class="agency-dashboard__analytics-chart-container" id="analytics-distribution-chart">
                <!-- Chart will be rendered here -->
              </div>
              <div class="agency-dashboard__analytics-legend">
                <div class="agency-dashboard__analytics-legend-item">
                  <span class="agency-dashboard__analytics-legend-dot" style="background-color: #1a1a1a;"></span>
                  <span>New Faces</span>
                </div>
                <div class="agency-dashboard__analytics-legend-item">
                  <span class="agency-dashboard__analytics-legend-dot" style="background-color: #666;"></span>
                  <span>Developing</span>
                </div>
                <div class="agency-dashboard__analytics-legend-item">
                  <span class="agency-dashboard__analytics-legend-dot" style="background-color: #999;"></span>
                  <span>Professional</span>
                </div>
                <div class="agency-dashboard__analytics-legend-item">
                  <span class="agency-dashboard__analytics-legend-dot" style="background-color: #ccc;"></span>
                  <span>Top Talent</span>
                </div>
              </div>
            </div>
          </div>

          <!-- Status Breakdown -->
          <div class="agency-dashboard__analytics-section">
            <h3 class="agency-dashboard__analytics-section-title">Applications by Status</h3>
            <div class="agency-dashboard__analytics-status-grid" id="analytics-status-grid">
              <!-- Populated by JavaScript -->
            </div>
          </div>

          <!-- Match Score Distribution -->
          <div class="agency-dashboard__analytics-section">
            <h3 class="agency-dashboard__analytics-section-title">Match Score Distribution</h3>
            <div class="agency-dashboard__analytics-scores" id="analytics-scores">
              <!-- Populated by JavaScript -->
            </div>
          </div>

          <!-- Applications by Board -->
          <div class="agency-dashboard__analytics-section">
            <h3 class="agency-dashboard__analytics-section-title">Applications by Board</h3>
            <div class="agency-dashboard__analytics-boards" id="analytics-boards">
              <!-- Populated by JavaScript -->
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
