{
  "name": "zipsite",
  "version": "1.0.0",
  "description": "ZipSite full-stack application",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon server.js",
    "migrate": "knex migrate:latest",
    "migrate:status": "knex migrate:status",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "test": "jest",
    "test:db": "node scripts/test-neon-connection.js",
    "build": "echo 'Build complete'",
    "build:migrate": "npm run migrate && npm run build"
  },
  "dependencies": {
    "@sparticuz/chromium": "^131.0.0",
    "bcrypt": "5.1.1",
    "connect-session-knex": "3.0.0",
    "dotenv": "16.4.5",
    "ejs": "3.1.10",
    "express": "4.19.2",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "7.0.0",
    "express-session": "1.17.3",
    "firebase": "^12.6.0",
    "firebase-admin": "^13.6.0",
    "helmet": "7.1.0",
    "knex": "^3.1.0",
    "multer": "1.4.5-lts.1",
    "pg": "^8.16.3",
    "puppeteer": "22.13.1",
    "qrcode": "^1.5.4",
    "serverless-http": "^3.2.0",
    "sharp": "0.33.4",
    "stripe": "^19.3.1",
    "uuid": "9.0.1",
    "validator": "13.11.0",
    "zod": "3.23.8"
  },
  "optionalDependencies": {
    "sqlite3": "5.1.7"
  },
  "devDependencies": {
    "jest": "29.7.0",
    "nodemon": "3.1.7",
    "supertest": "7.1.4"
  }
}
